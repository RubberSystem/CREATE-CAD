<UserControl x:Class="PluginEditor.UI.PluginEntryListItem" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<StackPanel Orientation="Horizontal">
		<Button Name="ButtonUpdate">
			<StackPanel Orientation="Horizontal">
				<Button Click="ButtonClick" BorderThickness="0">
					<Image Width="16" Height="16" Visibility="Visible" Name="updateAvailableImg" />
				</Button>
			</StackPanel>
		</Button>

		<TextBlock Text="TestModellingPlugin" Width="180" VerticalAlignment="Center" Name="TextName" />
		<CheckBox VerticalAlignment="Center" Name="CheckEnabled" Click="CheckEnabledClick" />
	</StackPanel>
</UserControl>