<Window x:Class="TestBuilderPlugin.View.BooHelpWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Height="500" Width="600" WindowStyle="SingleBorderWindow" ResizeMode="NoResize">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"></RowDefinition>
			<RowDefinition Height="40"></RowDefinition>
		</Grid.RowDefinitions>

		<Button x:Name="BackButton" Content="Back" Click="BackButtonClick" Grid.Row="1" Height="30" Width="40" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10,0,0,0"></Button>
		<Button x:Name="ForwardButton" Content="Fwd" Click="ForwardButtonClick" Grid.Row="1" Height="30" Width="40" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="60,0,0,0"></Button>
		<Button x:Name="CloseButton" Content="Close" Click="CloseButtonClick" Grid.Row="1" Height="30" Width="60" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,10,0"></Button>

		<WebBrowser x:Name="WebBrowser" Grid.Row="0" Height="420" Width="578"></WebBrowser>
	</Grid>
</Window>