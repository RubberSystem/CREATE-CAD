<Window x:Class="ErrorReportCommon.Messages.WarningWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="WarningWindow" Height="300" Width="300">
	<StackPanel Orientation="Vertical">
		<TextBlock Name="textBlock" />
		<StackPanel Orientation="Horizontal">
			<Button Margin="10, 2, 2, 0">
				<TextBlock Text="Report Bug..." />
			</Button>
			<Button Margin="150, 2, 2, 0">
				<TextBlock Margin="10, 2, 10, 0" Text="OK" />
			</Button>
		</StackPanel>
	</StackPanel>
</Window>